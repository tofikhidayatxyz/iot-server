{
  "name": "iot-server",
  "version": "0.0.0",
  "private": false,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon & npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "serve": "node_modules/live-server/live-server.js --middleware=./live-server.js --open=/dist/",
    "compile": "./bin/run.js --src   resources/views --dest dist --watch",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "dependencies": {
    "acorn": "^6.2.0",
    "bcrypt": "^3.0.6",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "debug": "~4.1.1",
    "dotenv": "^8.0.0",
    "express": "~4.17.3",
    "express-csurf": "^1.10.1",
    "express-edge": "^1.0.0",
    "express-minify-html": "^0.12.0",
    "fs": "0.0.2",
    "http-errors": "^1.7.3",
    "moment-timezone": "^0.5.26",
    "morgan": "~1.9.1",
    "mysql": "^2.17.1",
    "sweetalert2": "^8.13.1"
  },
  "devDependencies": {
    "bootstrap": "^4.3.1",
    "cross-env": "^5.2.0",
    "laravel-mix": "^4.0.16",
    "line-awesome": "^1.0.2",
    "popper.js": "^1.15.0",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.22.3",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "description": "examle",
  "main": "bin/www",
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tofikhidayat/iot-server.git"
  },
  "keywords": [
    "iot"
  ],
  "author": {
    "name": "tofik hidayat",
    "email": "tofik@syntac.co.id",
    "url": "tofikhidayat.xyz"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tofikhidayat/iot-server/issues"
  },
  "homepage": "https://github.com/tofikhidayat/iot-server#readme"
}
